#include <iostream>
#include <string>
std::string reverse_word(std::string str) {
  std::string str_out("", str.size());
  for (int i = str.size() - 1; i >= 0; --i) 
    str_out[str.size() - i - 1] = str[i];
  return str_out;
}
std::string reverse_words(std::string str)
{
  str += " ";
  std::string str_out = "";
  std::string word = "";
  for (size_t i = 0; i < str.size(); ++i) {
    if (str[i] != ' ')
      word.push_back(str[i]);
    else {
      if (i == str.size() - 1)
        str_out += reverse_word(word);
      else 
        str_out += reverse_word(word) + " ";
      word = "";
    }
  }
  return str_out;
}
